{{#if priorityInput}}
  <label><code>keyboardPriority</code>:</label>
  {{input value=keyboardPriority classNames="input-field"
    data-test-counter-priority-input=true}}
{{/if}}

{{#if activatedToggle}}
  <label><code>keyboardActivated</code>:</label>
  {{input type="checkbox" checked=toggleActivated classNames="checkbox"
    data-test-counter-activated-toggle=true}}
{{/if}}

{{#if firstResponderToggle}}
  <label><code>keyboardFirstResponder</code>:</label>
  {{input type="checkbox" checked=keyboardFirstResponder classNames="checkbox"
    data-test-counter-first-responder-toggle=true}}
{{/if}}

{{#if laxPriorityToggle}}
  <label><code>keyboardLaxPriority</code>:</label>
  {{input type="checkbox" checked=keyboardLaxPriority classNames="checkbox"
    data-test-counter-lax-priority-toggle=true}}
{{/if}}

{{#if stopImmediatePropagationToggle}}
  <label><code>stopImmediatePropagation()</code>:</label>
  {{input type="checkbox" checked=stopImmediatePropagation classNames="checkbox"
    data-test-counter-stop-immediate-propagation-toggle=true}}
{{/if}}

{{#if stopPropagationToggle}}
  <label><code>stopPropagation()</code>:</label>
  {{input type="checkbox" checked=stopPropagation classNames="checkbox"
    data-test-counter-stop-propagation-toggle=true}}
{{/if}}

<div data-test-counter-counter>{{counter}}</div>
